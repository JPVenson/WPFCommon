<Window x:Class="WpfApplication2.Windows.ActionTriggerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:windows="clr-namespace:WpfApplication2.Windows"
        mc:Ignorable="d"
        Title="ActionTriggerWindow" Height="300" Width="300" d:DataContext="{d:DesignInstance windows:ActionTriggerViewModel}">
    <StackPanel>
        <Button Command="{Binding ClearCommand}" Content="Clear"></Button>
        <Button Command="{Binding PullItemsAgainCommand}" Content="Reload"></Button>
    </StackPanel>
</Window>
